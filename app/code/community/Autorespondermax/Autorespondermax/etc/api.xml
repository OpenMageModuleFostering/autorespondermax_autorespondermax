        <methods>
          <list module="autorespondermax" translate="title">
            <title>Retrieve carts</title>
            <method>items</method>
            <acl>autorespondermax/sales_quote/list</acl>
          </list>
          <ids module="autorespondermax" translate="title">
            <title>Retrieve IDs</title>
            <method>ids</method>
            <acl>autorespondermax/sales_quote/ids</acl>
          </ids>
        </methods>
        <faults module="autorespondermax">
          <store_id_invalid>
            <code>101</code>
            <message>Store Id not found</message>
          </store_id_invalid>
          <filters_invalid>
            <code>102</code>
            <message>Invalid filters specified. Details in error message.</message>
          </filters_invalid>
          <page_out_of_range>
            <code>103</code>
            <message>Page out of range</message>
          </page_out_of_range>
        </faults>
      </autorespondermax_sales_quote>
