      <autorespondermax_general module="autorespondermax" translate="title">
        <title>Autoresponder Max</title>
        <acl>autorespondermax/general</acl>
        <model>autorespondermax/general_api</model>
        <methods>
          <info module="autorespondermax" translate="title">
            <title>Get info about current Magento installation</title>
            <method>info</method>
            <acl>autorespondermax/general/info</acl>
          </info>
        </methods>
      </autorespondermax_general>
